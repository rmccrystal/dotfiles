{
   "background": {
      "persistent": false,
      "scripts": [ "ScratchDeviceHost-Background.js" ]
   },
   "description": "Enables Scratch hardware extensions on Chrome. Requires the Scratch Device Plugin.",
   "externally_connectable": {
      "matches": [ "*://scratch.mit.edu/projects/*", "*://scratchx.org/*", "*://scratch.ly/*", "*://*.scratch.ly/*", "*://staging.scratch.mit.edu/*" ]
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Scratch Device Plugin Helper",
   "permissions": [ "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2.1.0"
}
